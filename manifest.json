{
  "manifest_version": 2,
  "version": "1.0",
  "minimum_chrome_version": "63",
  "name": "AutoPagerize",
  "description": "__MSG_description__",
  "homepage_url": "https://github.com/tophf/autopagerize_for_chrome",
  "default_locale": "en",
  "background": {
    "scripts": [
      "util/common.js",
      "bg/bg.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "options_ui": {
    "page": "ui/options.html",
    "chrome_style": false
  },
  "page_action": {
    "default_popup": "ui/popup.html",
    "default_icon": {
      "16": "icons/off/icon16.png",
      "32": "icons/off/icon32.png"
    }
  },
  "commands": {
    "_execute_page_action": {},
    "onOff": {
      "description": "__MSG_onOff__"
    },
    "loadMore01": {
      "description": "__MSG_loadMore__ (1)"
    },
    "loadMore02": {
      "description": "__MSG_loadMore__ (2)"
    },
    "loadMore05": {
      "description": "__MSG_loadMore__ (5)"
    },
    "loadMore10": {
      "description": "__MSG_loadMore__ (10)"
    }
  },
  "permissions": [
    "*://wedata.net/",
    "<all_urls>",
    "webNavigation",
    "contextMenus",
    "storage",
    "tabs"
  ],
  "key": ""
}
