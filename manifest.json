{
  "manifest_version": 2,
  "version": "1.0",
  "minimum_chrome_version": "63",
  "name": "AutoPagerize",
  "description": "__MSG_description__",
  "default_locale": "en",
  "background": {
    "scripts": [
      "util/common.js",
      "bg/bg.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "options_ui": {
    "page": "ui/options.html"
  },
  "page_action": {
    "default_popup": "ui/popup.html",
    "default_icon": {
      "16": "icons/off/icon16.png",
      "32": "icons/off/icon32.png"
    }
  },
  "permissions": [
    "*://wedata.net/",
    "<all_urls>",
    "webNavigation",
    "storage",
    "tabs"
  ]
}
